RUN: %ff -f 0x7fc00000 | FileCheck %s
CHECK: 64-bit double precision NaN
CHECK-NEXT: Hex: 0x7ff8000000000000
CHECK-NEXT: Bits: 0 11111111111 1000000000000000000000000000000000000000000000000000
CHECK-NEXT: Float: nan, nan, nan

CHECK: 32-bit single precision NaN
CHECK-NEXT: Hex: 0x7fc00000
CHECK-NEXT: Bits: 0 11111111 10000000000000000000000
CHECK-NEXT: Float: nan, nan, nan

CHECK: 16-bit half precision NaN
CHECK-NEXT: Hex: 0x7fff
CHECK-NEXT: Bits: 0 11111 1111111111
CHECK-NEXT: Float: nan, nan, nan
